generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}


// 3. Data Model
model Log {
  id          Int      @id @default(autoincrement())
  date        String
  entryNumber Int
  week        String
  topic       String
  description String
  createdAt   DateTime @default(now())
}